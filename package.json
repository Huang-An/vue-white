{
  "name": "vue-white",
  "version": "1.0.0",
  "description": "A Mobile Component Library for Vue.js",
  "main": "lib/white.common.js",
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build:entry": "node build/bin/build-entry.js",
    "build:theme": "node build/bin/build-theme.js && gulp build --gulpfile theme/gulpfile.js && cp-cli theme/lib lib/theme",
    "build:common": "webpack --config build/webpack.common.js",
    "build:component": "webpack --config build/webpack.component.js",
    "dist": "npm run build:entry && npm run build:theme && npm run build:common && npm run build:component",
    "lint": "eslint src/**/* packages/**/* --quiet"
  },
  "author": "huangAn",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.6.0",
    "cp-cli": "^1.1.2",
    "css-loader": "^0.28.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.6",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "json-templater": "^1.2.0",
    "node-sass": "^4.11.0",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^1.1.2",
    "vue": "^2.6.6",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.5.22",
    "vue-template-es2015-compiler": "^1.8.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
